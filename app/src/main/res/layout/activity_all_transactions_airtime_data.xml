<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="allTransactionsAirtimeDataVM" type="com.huiospayapplication.app.modules.alltransactionsairtimedata.data.viewmodel.AllTransactionsAirtimeDataVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackrectangle5793"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <FrameLayout android:id="@+id/frameAllTransaction"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylegray_100">
            <ImageView android:id="@+id/imageRectangle5793"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_109pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_rectangle5793_1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle5793_1"/>
            <LinearLayout android:id="@+id/linearColumnmenu"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_33pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="center_horizontal"
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
