                            android:layout_height="@dimen/_10pxh"
                            android:layout_marginBottom="@dimen/_7pxv"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_menu_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_menu_white_a700"/>
                        <LinearLayout
                            android:id="@+id/linearRowownyourweird"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_170pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.88"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <LinearLayout android:gravity="end"
                                android:id="@+id/linearColumnownyourweird"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.83"
                                android:layout_width="0dp" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:gravity="start"
                                    android:id="@+id/txtOWNYOURWEIRD"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_1pxh"
                                    android:singleLine="true"
                                    android:text="@{allTransactionsAirtimeDataVM.allTransactionsAirtimeDataModel.txtOWNYOURWEIRD}"
                                    style="@style/txtWorkSanssemibold12_2" tools:text="@string/lbl_beetle_taxi_inc"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtAcctNo001122"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_1pxh"
                                    android:layout_marginStart="@dimen/_16pxh"
                                    android:layout_marginTop="@dimen/_3pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_1pxh"
                                    android:singleLine="true"
                                    android:text="@{allTransactionsAirtimeDataVM.allTransactionsAirtimeDataModel.txtAcctNo001122}"
                                    style="@style/txtWorkSansregular10_1" tools:text="@string/msg_acct_no_001122"/>
                            </LinearLayout>
                            <ImageView
                                android:id="@+id/imageIconsaxLinear"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_11pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_20pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_iconsaxlinear_white_a700"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_iconsaxlinear_white_a700"/>
                        </LinearLayout>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnownyourweirdOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_6">
                    <LinearLayout
                        android:id="@+id/linearRowownyourweirdOne"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylecornerRadius_6">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtOWNYOURWEIRDOne"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.58"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{allTransactionsAirtimeDataVM.allTransactionsAirtimeDataModel.txtOWNYOURWEIRDOne}"
                            style="@style/txtWorkSanssemibold24_1" tools:text="@string/msg_all_transaction"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearDropdown"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_22pxh"
                            android:layout_weight="0.42"
                            android:layout_width="0dp"
                            android:orientation="vertical" style="@style/groupStylecornerRadius_6">
                            <Spinner
                                android:background="@drawable/layer_list_bg_white_a700_radius_10"
                                android:gravity="start"
                                android:id="@+id/spinnerFrame1656"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="center_horizontal"
                    android:id="@+id/recyclerListiconsaxlinearOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_41pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_25pxh}" tools:itemCount="3" tools:listitem="@layout/row_listiconsaxlinear_one"/>
                <View android:background="@color/bluegray_100"
                    android:id="@+id/lineLineEight"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_442pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_13pxv" android:layout_width="@dimen/_295pxh"/>
            </LinearLayout>
        </FrameLayout>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/floatingBtnFloatingactionbutton"
            android:layout_gravity="bottom|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_146pxh"
            android:layout_marginStart="@dimen/_146pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="wrap_content"
            app:fabCustomSize="@dimen/_63pxh"
            app:srcCompat="@drawable/img_iconsaxlinear_white_a700_63x63" app:tint="@color/white_A700"/>
    </FrameLayout>
</layout>
