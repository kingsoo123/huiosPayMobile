<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="enterBvnVM" type="com.huiospayapplication.app.modules.enterbvn.data.viewmodel.EnterBvnVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumngroupone"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameStackback"
            android:layout_height="@dimen/_277pxv" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStackgroupone"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_254pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent" style="@style/groupStylecornerRadius_2">
                <ImageView android:id="@+id/imageGroupOne"
                    android:layout_gravity="top|end"
                    android:layout_height="@dimen/_150pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_97pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group1"/>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearColumnback"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_30pxv"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="center_horizontal"
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearRowback"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtBack"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_weight="0.63"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{enterBvnVM.enterBvnModel.txtBack}"
                            style="@style/txtWorkSanssemibold12_2" tools:text="@string/lbl_back"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtThirteen"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_249pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.37"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{enterBvnVM.enterBvnModel.txtThirteen}"
                            style="@style/txtWorkSansregular12_1" tools:text="@string/lbl_1_3"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <ProgressBar android:gravity="center_horizontal"
                    android:id="@+id/progressBarGroupNine"
                    android:indeterminate="false"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:max="100" android:minHeight="@dimen/_4pxv"
                    android:progress="30"
                    android:progressDrawable="@drawable/layer_list_radius_2" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEnteryourBVN"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_55pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{enterBvnVM.enterBvnModel.txtEnteryourBVN}"
                    style="@style/txtWorkSanssemibold24_1" tools:text="@string/lbl_enter_your_bvn"/>
                <TextView android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="match_parent"
                    android:text="@{enterBvnVM.enterBvnModel.txtDescription}"
                    style="@style/txtWorkSansregular12_1" tools:text="@string/msg_please_enter_yo"/>
            </LinearLayout>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_gray_400_radius_24"
                android:ellipsize="end" android:hint="@string/lbl_bvn"
                android:id="@+id/etGroupFour" android:inputType="text"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_21pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@={enterBvnVM.enterBvnModel.etGroupFourValue}"
                android:textColorHint="@color/gray_500"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
        </FrameLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearEnterBVN"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <LinearLayout android:id="@+id/linearRowdateofbirth"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_35pxh"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadius">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDateofBirth"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_weight="0.79"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{enterBvnVM.enterBvnModel.txtDateofBirth}"
                    style="@style/txtWorkSansmedium12" tools:text="@string/lbl_date_of_birth"/>
                <ImageView android:id="@+id/imageCalendar"
                    android:layout_height="@dimen/_21pxh"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_152pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_calendar"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_calendar"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtPleaseinput"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_35pxh"
                android:layout_marginStart="@dimen/_35pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{enterBvnVM.enterBvnModel.txtPleaseinput}"
                style="@style/txtWorkSansregular10" tools:text="@string/msg_please_input"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumndescriptionOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_34pxh"
                android:layout_marginStart="@dimen/_35pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecyan_50cornerRadius">
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtDescriptionOne"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginEnd="@dimen/_51pxh"
                    android:layout_marginStart="@dimen/_51pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:text="@{enterBvnVM.enterBvnModel.txtDescriptionOne}"
                    style="@style/txtWorkSansregular12" tools:text="@string/msg_dial_565_0_on"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameStackgroup"
                android:layout_height="@dimen/_241pxv"
                android:layout_marginTop="@dimen/_109pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageGroup"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_219pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_37pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_219x37"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group_219x37"/>
                <ImageView android:id="@+id/imageGroupOne1"
                    android:layout_gravity="top|end"
                    android:layout_height="@dimen/_219pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="@dimen/_36pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_219x36"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group_219x36"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnNext"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_40pxv"
                    android:layout_marginEnd="@dimen/_34pxh"
                    android:layout_marginStart="@dimen/_34pxh"
                    android:layout_marginTop="@dimen/_40pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_next"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_1" tools:text="@string/lbl_next"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
