<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpOneVM" type="com.huiospayapplication.app.modules.signupone.data.viewmodel.SignUpOneVM"/>
    </data>
    <LinearLayout android:id="@+id/linearSIgnUpOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <TextView android:ellipsize="end" android:id="@+id/txtBack"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{signUpOneVM.signUpOneModel.txtBack}"
            style="@style/txtWorkSanssemibold12" tools:text="@string/lbl_back"/>
        <ImageView android:id="@+id/imageGroupTwentyEight"
            android:layout_gravity="end"
            android:layout_height="@dimen/_275pxh"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="@dimen/_304pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_group28_275x304"
            tools:ignore="ContentDescription" tools:src="@drawable/img_group28_275x304"/>
        <TextView android:id="@+id/txtCreateAccount"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_34pxh"
            android:layout_marginStart="@dimen/_34pxh"
            android:layout_marginTop="@dimen/_39pxv"
            android:layout_width="wrap_content"
            android:text="@{signUpOneVM.signUpOneModel.txtCreateAccount}"
            style="@style/txtWorkSanssemibold36" tools:text="@string/lbl_create_account"/>
        <TextView android:ellipsize="end" android:gravity="start"
            android:id="@+id/txtProvideyourem"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_34pxh"
            android:layout_marginStart="@dimen/_34pxh"
            android:layout_marginTop="@dimen/_6pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{signUpOneVM.signUpOneModel.txtProvideyourem}"
            style="@style/txtWorkSansregular12" tools:text="@string/msg_provide_your_em"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowphonenumber"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadius">
            <LinearLayout android:id="@+id/linearRowrectangleseven"
                android:layout_height="wrap_content"
                android:layout_weight="0.5" android:layout_width="0dp"
                android:orientation="horizontal" style="@style/groupStylewhite_1">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnrectangleseven"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_weight="0.27"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStyleteal_901">
                    <View android:background="@color/white_A700"
                        android:id="@+id/viewRectangleSeven"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_11pxh"
                        android:layout_marginEnd="@dimen/_4pxh"
                        android:layout_marginStart="@dimen/_5pxh" android:layout_width="@dimen/_5pxh"/>
                </LinearLayout>
                <Spinner android:id="@+id/spinnerGroup109"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_weight="0.73"
                    android:layout_width="0dp" tools:listitem="@layout/spinner_item"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPhoneNumber"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_95pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_weight="0.5" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{signUpOneVM.signUpOneModel.txtPhoneNumber}"
                style="@style/txtWorkSansmedium12" tools:text="@string/lbl_phone_number"/>
        </LinearLayout>
        <androidx.appcompat.widget.SearchView
            android:background="@drawable/rectangle_bg_white_a700_border_gray_400_radius_24"
            android:gravity="center_horizontal"
            android:id="@+id/searchViewGroupEleven"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:theme="@style/searchViewGroupEleven"
            app:iconifiedByDefault="false"
            app:queryBackground="@android:color/transparent"
            app:queryHint="@string/lbl_password" app:searchIcon="@drawable/img_search"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtUseemailaddre"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{signUpOneVM.signUpOneModel.txtUseemailaddre}"
            style="@style/txtWorkSanssemibold12_1" tools:text="@string/msg_use_email_addre"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnNext"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_15pxv" android:singleLine="true"
            android:text="@string/lbl_next" android:textAllCaps="false"
            style="@style/btnSolidRounded_1" tools:text="@string/lbl_next"/>
        <TextView android:ellipsize="end"
            android:gravity="center_horizontal"
            android:id="@+id/txtConfirmation"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{signUpOneVM.signUpOneModel.txtConfirmation}"
            style="@style/txtWorkSansregular12" tools:text="@string/msg_already_have_an"/>
    </LinearLayout>
</layout>
