<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="billPaymentSelectCategoryVM" type="com.huiospayapplication.app.modules.billpaymentselectcategory.data.viewmodel.BillPaymentSelectCategoryVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackrectangle5793"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_100">
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_gravity="start" android:layout_height="0dp"
            android:layout_weight="1" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameBillPayment"
                android:layout_height="match_parent" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageRectangle5793"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_109pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_rectangle5793_13}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle5793_13"/>
                <LinearLayout android:id="@+id/linearColumnownyourweird"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_60pxv"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_60pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtOWNYOURWEIRD"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_88pxh"
                        android:layout_marginStart="@dimen/_88pxh"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{billPaymentSelectCategoryVM.billPaymentSelectCategoryModel.txtOWNYOURWEIRD}"
                        style="@style/txtWorkSanssemibold24_1" tools:text="@string/lbl_bill_payment"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEnteranAccoun"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_55pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{billPaymentSelectCategoryVM.billPaymentSelectCategoryModel.txtEnteranAccoun}"
                        style="@style/txtWorkSanssemibold20" tools:text="@string/lbl_select_category"/>
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/recyclerListlock"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_36pxv"
                        android:layout_width="match_parent"
                        android:nestedScrollingEnabled="false"
                        android:orientation="vertical"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        itemSpace="@{@dimen/_22pxh}" tools:itemCount="3" tools:listitem="@layout/row_listlock"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnNext"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_88pxh"
                        android:layout_marginStart="@dimen/_88pxh"
                        android:layout_marginTop="@dimen/_145pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:paddingBottom="@dimen/_9pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_9pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_next"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_2" tools:text="@string/lbl_next"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageGroup"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_219pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_37pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_47"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group_47"/>
                <ImageView android:id="@+id/imageGroupOne"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_219pxh"
                    android:layout_marginBottom="@dimen/_23pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="@dimen/_36pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_48"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group_48"/>
            </FrameLayout>
        </androidx.core.widget.NestedScrollView>
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowmenu"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageMenu"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_menu_white_a700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_menu_white_a700"/>
                <LinearLayout android:id="@+id/linearRowownyourweirdOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_170pxh"
                    android:layout_weight="0.88"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <LinearLayout android:gravity="end"
                        android:id="@+id/linearColumnownyourweirdOne"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.83"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtOWNYOURWEIRDOne"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{billPaymentSelectCategoryVM.billPaymentSelectCategoryModel.txtOWNYOURWEIRDOne}"
                            style="@style/txtWorkSanssemibold12_2" tools:text="@string/lbl_beetle_taxi_inc"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAcctNo001122"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_1pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{billPaymentSelectCategoryVM.billPaymentSelectCategoryModel.txtAcctNo001122}"
                            style="@style/txtWorkSansregular10_1" tools:text="@string/msg_acct_no_001122"/>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameStackcalendar"
                        android:layout_height="@dimen/_20pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_20pxh">
                        <ImageView android:id="@+id/imageCalendar"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_calendar_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_calendar_white_a700"/>
                        <ImageView android:id="@+id/imageTrash"
                            android:layout_gravity="bottom|center"
                            android:layout_height="@dimen/_10pxh"
                            android:layout_marginEnd="@dimen/_6pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_8pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_trash_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_trash_white_a700"/>
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
    </FrameLayout>
</layout>
