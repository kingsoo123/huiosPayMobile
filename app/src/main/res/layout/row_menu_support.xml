<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="menuSupportRowModel" type="com.huiospayapplication.app.modules.menusupport.data.model.MenuSupportRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowiconsaxlinear"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageIconsaxLinear"
            android:layout_height="@dimen/_29pxh"
            android:layout_marginBottom="@dimen/_3pxv"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="@dimen/_29pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_iconsaxlinear_light_blue_700"
            tools:ignore="ContentDescription" tools:src="@drawable/img_iconsaxlinear_light_blue_700"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnenterreceipien"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_28pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_weight="0.66" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtEnterReceipien"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:singleLine="true"
                android:text="@{menuSupportRowModel.txtEnterReceipien}"
                style="@style/txtWorkSansmedium16" tools:text="@string/lbl_dispute"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtEnterReceipienOne"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:singleLine="true"
                android:text="@{menuSupportRowModel.txtEnterReceipienOne}"
                style="@style/txtWorkSansregular12_2" tools:text="@string/msg_get_your_disput"/>
        </LinearLayout>
    </LinearLayout>
</layout>
