<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="posTerminalDetailsVM" type="com.huiospayapplication.app.modules.posterminaldetails.data.viewmodel.PosTerminalDetailsVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_100">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumn"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecornerRadius_9">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_26pxv"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnmenu"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:id="@+id/linearRowmenu"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMenu"
                            android:layout_height="@dimen/_10pxh"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_menu_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_menu_white_a700"/>
                        <LinearLayout
                            android:id="@+id/linearRowownyourweird"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_170pxh"
                            android:layout_weight="0.88"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <LinearLayout android:gravity="end"
                                android:id="@+id/linearColumnownyourweird"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.83"
                                android:layout_width="0dp" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:gravity="start"
                                    android:id="@+id/txtOWNYOURWEIRD"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_1pxh"
                                    android:singleLine="true"
                                    android:text="@{posTerminalDetailsVM.posTerminalDetailsModel.txtOWNYOURWEIRD}"
                                    style="@style/txtWorkSanssemibold12_2" tools:text="@string/lbl_beetle_taxi_inc"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtAcctNo001122"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_1pxh"
                                    android:layout_marginStart="@dimen/_16pxh"
                                    android:layout_marginTop="@dimen/_3pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_1pxh"
                                    android:singleLine="true"
                                    android:text="@{posTerminalDetailsVM.posTerminalDetailsModel.txtAcctNo001122}"
                                    style="@style/txtWorkSansregular10_1" tools:text="@string/msg_acct_no_001122"/>
                            </LinearLayout>
                            <FrameLayout
                                android:id="@+id/frameStackcalendar"
                                android:layout_height="@dimen/_20pxv"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_11pxh"
                                android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_20pxh">
                                <ImageView
                                    android:id="@+id/imageCalendar"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_20pxh"
                                    android:layout_width="@dimen/_20pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_calendar_white_a700"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_calendar_white_a700"/>
                                <ImageView android:id="@+id/imageTrash"
                                    android:layout_gravity="bottom|center"
                                    android:layout_height="@dimen/_10pxh"
                                    android:layout_marginEnd="@dimen/_6pxh"
                                    android:layout_marginStart="@dimen/_5pxh"
                                    android:layout_marginTop="@dimen/_10pxv"
                                    android:layout_width="@dimen/_8pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_trash_white_a700"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_trash_white_a700"/>
                            </FrameLayout>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowarrowleft"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_28pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="@dimen/_7pxh"
                            android:layout_marginBottom="@dimen/_7pxv"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="@dimen/_3pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtBack"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.16"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{posTerminalDetailsVM.posTerminalDetailsModel.txtBack}"
                            style="@style/txtWorkSansmedium12_3" tools:text="@string/lbl_back"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtOWNYOURWEIRDOne"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_36pxh"
                            android:layout_weight="0.82"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{posTerminalDetailsVM.posTerminalDetailsModel.txtOWNYOURWEIRDOne}"
                            style="@style/txtWorkSanssemibold24_1" tools:text="@string/lbl_pos_terminal"/>
                    </LinearLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearColumnenteranaccoun"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_100">
            <TextView android:ellipsize="end"
                android:id="@+id/txtEnteranAccoun"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{posTerminalDetailsVM.posTerminalDetailsModel.txtEnteranAccoun}"
                style="@style/txtWorkSanssemibold20" tools:text="@string/lbl_details"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumndescription"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecyan_1">
                <TextView android:gravity="start"
                    android:id="@+id/txtDescription"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_21pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:text="@{posTerminalDetailsVM.posTerminalDetailsModel.txtDescription}"
                    style="@style/txtWorkSansregular16" tools:text="@string/msg_terminal_id_20"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowiconsaxlinear"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_83pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylelight_blue_900cornerRadius">
                <ImageView android:id="@+id/imageIconsaxLinear"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginStart="@dimen/_60pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconsaxlinear_white_a700_20x19"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconsaxlinear_white_a700_20x19"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtChangeTerminal"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_58pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.86"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{posTerminalDetailsVM.posTerminalDetailsModel.txtChangeTerminal}"
                    style="@style/txtWorkSansmedium12_5" tools:text="@string/msg_change_terminal"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowiconsaxlinearOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylelight_blue_900cornerRadius">
                <ImageView android:id="@+id/imageIconsaxLinearOne"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconsaxlinear_20x20"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconsaxlinear_20x20"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtWithdrawalAuth"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.9"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{posTerminalDetailsVM.posTerminalDetailsModel.txtWithdrawalAuth}"
                    style="@style/txtWorkSansmedium12_5" tools:text="@string/msg_withdrawal_auth"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowgroup"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageGroup"
                    android:layout_height="@dimen/_219pxh"
                    android:layout_marginTop="@dimen/_57pxv"
                    android:layout_width="@dimen/_37pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_69"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group_69"/>
                <LinearLayout
                    android:id="@+id/linearRowiconsaxlinearTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_239pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_weight="0.78"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStylelight_blue_900cornerRadius">
                    <ImageView android:id="@+id/imageIconsaxLinearTwo"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginStart="@dimen/_56pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_iconsaxlinear_2"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_iconsaxlinear_2"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtReceiptCOnfigu"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_55pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="0.86"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{posTerminalDetailsVM.posTerminalDetailsModel.txtReceiptCOnfigu}"
                        style="@style/txtWorkSansmedium12_5" tools:text="@string/msg_receipt_configu"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageGroupOne"
                    android:layout_height="@dimen/_219pxh"
                    android:layout_marginBottom="@dimen/_23pxv"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_34pxv"
                    android:layout_width="@dimen/_36pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_70"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group_70"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
