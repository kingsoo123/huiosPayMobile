                    android:background="@drawable/rectangle_bg_cyan_902_radius_11_94"
                    android:id="@+id/btnVolume"
                    android:layout_height="@dimen/_23pxh"
                    android:layout_marginBottom="@dimen/_36pxv"
                    android:layout_width="@dimen/_23pxh"
                    android:paddingHorizontal="@dimen/_4pxh"
                    android:paddingVertical="@dimen/_4pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_volume_white_a700_23x23" tools:ignore="ContentDescription"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumn"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.92"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_8">
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumndescription"
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylelight_blue_2">
                        <TextView android:gravity="start"
                            android:id="@+id/txtDescription"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="@dimen/_15pxv"
                            android:layout_marginEnd="@dimen/_19pxh"
                            android:layout_marginStart="@dimen/_19pxh"
                            android:layout_marginTop="@dimen/_15pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:text="@{supportChatWithUsVM.supportChatWithUsModel.txtDescription}"
                            style="@style/txtWorkSansmedium12_6" tools:text="@string/msg_hi_there_thank"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end" android:gravity="end"
                android:id="@+id/txtEnterAccountN"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_178pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:singleLine="true"
                android:text="@{supportChatWithUsVM.supportChatWithUsModel.txtEnterAccountN}"
                style="@style/txtWorkSansmedium12" tools:text="@string/msg_select_if_the_s"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnKycaccountVerification"
                android:layout_gravity="end"
                android:layout_height="@dimen/_37pxv"
                android:layout_marginEnd="@dimen/_26pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="@dimen/_215pxh"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingEnd="@dimen/_25pxh"
                android:paddingStart="@dimen/_29pxh"
                android:paddingTop="@dimen/_10pxv"
                android:singleLine="true"
                android:text="@string/msg_kyc_account_ver"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_7" tools:text="@string/msg_kyc_account_ver"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnRequestPosTerminal"
                android:layout_gravity="end"
                android:layout_height="@dimen/_37pxv"
                android:layout_marginEnd="@dimen/_26pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="@dimen/_192pxh"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingEnd="@dimen/_24pxh"
                android:paddingStart="@dimen/_24pxh"
                android:paddingTop="@dimen/_10pxv"
                android:singleLine="true"
                android:text="@string/msg_request_pos_ter"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_7" tools:text="@string/msg_request_pos_ter"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnAddCashdepositOne"
                android:layout_gravity="end"
                android:layout_height="@dimen/_37pxv"
                android:layout_marginEnd="@dimen/_26pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="@dimen/_168pxh"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingEnd="@dimen/_24pxh"
                android:paddingStart="@dimen/_26pxh"
                android:paddingTop="@dimen/_10pxv"
                android:singleLine="true"
                android:text="@string/msg_add_cash_deposi"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_7" tools:text="@string/msg_add_cash_deposi"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnCashOutwithdrawal"
                android:layout_gravity="end"
                android:layout_height="@dimen/_37pxv"
                android:layout_marginEnd="@dimen/_26pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="@dimen/_192pxh"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingEnd="@dimen/_24pxh"
                android:paddingStart="@dimen/_27pxh"
                android:paddingTop="@dimen/_10pxv"
                android:singleLine="true"
                android:text="@string/msg_cash_out_withdr"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_7" tools:text="@string/msg_cash_out_withdr"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnResolveDisputeOne"
                android:layout_gravity="end"
                android:layout_height="@dimen/_37pxv"
                android:layout_marginEnd="@dimen/_26pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="@dimen/_155pxh"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingEnd="@dimen/_24pxh"
                android:paddingStart="@dimen/_26pxh"
                android:paddingTop="@dimen/_10pxv"
                android:singleLine="true"
                android:text="@string/lbl_resolve_dispute"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_7" tools:text="@string/lbl_resolve_dispute"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowwhatsapp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_33pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageWhatsapp"
                    android:layout_height="@dimen/_21pxh"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="@dimen/_22pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_whatsapp"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_whatsapp"/>
                <EditText
                    android:background="@drawable/rectangle_bg_gray_200_border_gray_601_radius_20"
                    android:ellipsize="end"
                    android:hint="@string/msg_type_message_he"
                    android:id="@+id/etGroup1695"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_weight="0.93"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:paddingBottom="@dimen/_14pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_23pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@={supportChatWithUsVM.supportChatWithUsModel.etGroup1695Value}"
                    android:textColorHint="@color/gray_500"
                    style="@style/etSolidRoundedOutline_7" tools:ignore="Autofill"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
