<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="proofOfIdentityVM" type="com.huiospayapplication.app.modules.proofofidentity.data.viewmodel.ProofOfIdentityVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumngroupone"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameStackback"
            android:layout_height="@dimen/_328pxv" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStackgroupone"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_200pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent" style="@style/groupStylecornerRadius_2">
                <ImageView android:id="@+id/imageGroupOne"
                    android:layout_gravity="top|end"
                    android:layout_height="@dimen/_116pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="@dimen/_97pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group1"/>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearColumnback"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_30pxv"
                android:layout_marginEnd="@dimen/_29pxh"
                android:layout_marginStart="@dimen/_29pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearRowback"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtBack"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_weight="0.6"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{proofOfIdentityVM.proofOfIdentityModel.txtBack}"
                            style="@style/txtWorkSanssemibold12_2" tools:text="@string/lbl_back"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtThirtyThree"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_249pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.4"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{proofOfIdentityVM.proofOfIdentityModel.txtThirtyThree}"
                            style="@style/txtWorkSansregular12_1" tools:text="@string/lbl_3_3"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <ProgressBar android:gravity="center_horizontal"
                    android:id="@+id/progressBarGroupNine"
                    android:indeterminate="false"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:max="100" android:minHeight="@dimen/_4pxv"
                    android:progress="98"
                    android:progressDrawable="@drawable/layer_list_radius_2" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtProofofIdenti"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_44pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{proofOfIdentityVM.proofOfIdentityModel.txtProofofIdenti}"
                    style="@style/txtWorkSanssemibold24_1" tools:text="@string/msg_proof_of_identi"/>
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_4pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="match_parent"
                    android:text="@{proofOfIdentityVM.proofOfIdentityModel.txtDescription}"
                    style="@style/txtWorkSansregular12_1" tools:text="@string/msg_please_upload_p"/>
            </LinearLayout>
            <ImageButton
                android:background="@drawable/rectangle_bg_gray_200_radius_48_5"
                android:id="@+id/btnIconsaxLinear"
                android:layout_gravity="bottom|end"
                android:layout_height="@dimen/_97pxh"
                android:layout_marginBottom="@dimen/_21pxv"
                android:layout_marginEnd="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="@dimen/_97pxh"
                android:paddingHorizontal="@dimen/_36pxh"
                android:paddingVertical="@dimen/_36pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_iconsaxlinear" tools:ignore="ContentDescription"/>
            <FrameLayout android:id="@+id/frameStackiconsaxlinearOne"
                android:layout_gravity="bottom|center"
                android:layout_height="@dimen/_139pxv"
                android:layout_marginEnd="@dimen/_110pxh"
                android:layout_marginStart="@dimen/_110pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_139pxh" style="@style/groupStylegray_200cornerRadius">
                <ImageView android:id="@+id/imageIconsaxLinearOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_33pxh"
                    android:layout_marginHorizontal="@dimen/_40pxh"
                    android:layout_marginVertical="@dimen/_40pxv"
                    android:layout_width="@dimen/_32pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_iconsaxlinear"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_iconsaxlinear"/>
            </FrameLayout>
        </FrameLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearProofofIdenti"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <TextView android:ellipsize="end"
                android:id="@+id/txtProfileImage"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{proofOfIdentityVM.proofOfIdentityModel.txtProfileImage}"
                style="@style/txtWorkSanssemibold16" tools:text="@string/lbl_profile_image"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_gray_400_radius_24"
                android:drawableEnd="@drawable/img_link"
                android:drawablePadding="@dimen/_30pxh"
                android:ellipsize="end"
                android:hint="@string/msg_upload_proof_of"
                android:id="@+id/etGroupEleven" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_21pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@={proofOfIdentityVM.proofOfIdentityModel.etGroupElevenValue}"
                android:textColorHint="@color/gray_500"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtProofofIdentiOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{proofOfIdentityVM.proofOfIdentityModel.txtProofofIdentiOne}"
                style="@style/txtWorkSansregular8" tools:text="@string/msg_proof_of_identi2"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_gray_400_radius_24"
                android:drawableEnd="@drawable/img_link"
                android:drawablePadding="@dimen/_30pxh"
                android:ellipsize="end"
                android:hint="@string/msg_upload_proof_of2"
                android:id="@+id/etGroupNineteen"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_21pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@={proofOfIdentityVM.proofOfIdentityModel.etGroupNineteenValue}"
                android:textColorHint="@color/gray_500"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <FrameLayout android:id="@+id/frameStackgroup"
                android:layout_height="@dimen/_252pxv"
                android:layout_marginTop="@dimen/_8pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageGroup"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_219pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_37pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_9"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group_9"/>
                <ImageView android:id="@+id/imageGroupOne1"
                    android:layout_gravity="top|end"
                    android:layout_height="@dimen/_219pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="@dimen/_36pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group_10"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group_10"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnContinue"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_40pxv"
                    android:layout_marginEnd="@dimen/_34pxh"
                    android:layout_marginStart="@dimen/_34pxh"
                    android:layout_marginTop="@dimen/_40pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_continue"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_1" tools:text="@string/lbl_continue"/>
                <TextView android:gravity="top|center"
                    android:id="@+id/txtTermAndCondition"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_59pxv"
                    android:layout_marginEnd="@dimen/_67pxh"
                    android:layout_marginStart="@dimen/_67pxh"
                    android:layout_marginTop="@dimen/_59pxv"
                    android:layout_width="match_parent"
                    android:text="@{proofOfIdentityVM.proofOfIdentityModel.txtTermAndCondition}"
                    style="@style/txtWorkSansregular12" tools:text="@string/msg_by_clicking_the3"/>
                <TextView android:gravity="top|start"
                    android:id="@+id/txtDescriptionOne"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_39pxh"
                    android:layout_marginStart="@dimen/_39pxh"
                    android:layout_width="match_parent"
                    android:text="@{proofOfIdentityVM.proofOfIdentityModel.txtDescriptionOne}"
                    style="@style/txtWorkSansregular8" tools:text="@string/msg_proof_of_person"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
