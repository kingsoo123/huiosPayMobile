<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="supportMessagesVM" type="com.huiospayapplication.app.modules.supportmessages.data.viewmodel.SupportMessagesVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearSupportMessa"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_100">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylecornerRadius_13">
            <LinearLayout android:id="@+id/linearRowshare"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_24pxv"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.92"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_7pxh"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_3pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtBack"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.23"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{supportMessagesVM.supportMessagesModel.txtBack}"
                        style="@style/txtWorkSansmedium12_3" tools:text="@string/lbl_back"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtOWNYOURWEIRD"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_67pxh"
                        android:layout_weight="0.74"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{supportMessagesVM.supportMessagesModel.txtOWNYOURWEIRD}"
                        style="@style/txtWorkSanssemibold20_2" tools:text="@string/lbl_messages"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageShare"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_89pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_share"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_share"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearRowgroup1701"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_44pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtGroup1701"
                android:layout_height="wrap_content"
                android:layout_weight="0.11" android:layout_width="0dp"
                android:paddingBottom="@dimen/_2pxv"
                android:paddingEnd="@dimen/_6pxh"
                android:paddingStart="@dimen/_8pxh"
                android:paddingTop="@dimen/_2pxv"
                android:singleLine="true"
                android:text="@{supportMessagesVM.supportMessagesModel.txtGroup1701}"
                style="@style/txtSolidRounded_5" tools:text="@string/lbl_h"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnenterreceipien"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_weight="0.87" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEnterReceipien"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@{supportMessagesVM.supportMessagesModel.txtEnterReceipien}"
                    style="@style/txtWorkSansmedium12_1" tools:text="@string/msg_hey_huiospay"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEnterReceipienOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_2pxh"
                    android:singleLine="true"
                    android:text="@{supportMessagesVM.supportMessagesModel.txtEnterReceipienOne}"
                    style="@style/txtWorkSansregular10_5" tools:text="@string/lbl_2dys_ago"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageArrowright"
                android:layout_height="@dimen/_8pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_4pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowright"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
        </LinearLayout>
        <View android:background="@color/gray_401"
            android:id="@+id/lineLineSeventyTwo"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_20pxv" android:layout_width="@dimen/_313pxh"/>
        <LinearLayout android:id="@+id/linearRowgroup1701One"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtGroup1701One"
                android:layout_height="wrap_content"
                android:layout_weight="0.11" android:layout_width="0dp"
                android:paddingBottom="@dimen/_2pxv"
                android:paddingEnd="@dimen/_11pxh"
                android:paddingStart="@dimen/_12pxh"
                android:paddingTop="@dimen/_2pxv"
                android:singleLine="true"
                android:text="@{supportMessagesVM.supportMessagesModel.txtGroup1701One}"
                style="@style/txtSolidRounded_5" tools:text="@string/lbl_i"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnenterreceipienTwo"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_weight="0.87" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEnterReceipienTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_3pxh"
                    android:singleLine="true"
                    android:text="@{supportMessagesVM.supportMessagesModel.txtEnterReceipienTwo}"
                    style="@style/txtWorkSansmedium12_1" tools:text="@string/msg_i_have_an_issue"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEnterReceipienThree"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_2pxh"
                    android:singleLine="true"
                    android:text="@{supportMessagesVM.supportMessagesModel.txtEnterReceipienThree}"
                    style="@style/txtWorkSansregular10_5" tools:text="@string/lbl_4dys_ago"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageArrowrightOne"
                android:layout_height="@dimen/_8pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_4pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowright"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
        </LinearLayout>
        <View android:background="@color/gray_401"
            android:id="@+id/lineLineSeventyFour"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_20pxv" android:layout_width="@dimen/_313pxh"/>
    </LinearLayout>
</layout>
