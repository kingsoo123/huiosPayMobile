<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listmaximize7RowModel" type="com.huiospayapplication.app.modules.alltransactionstransfersone.data.model.Listmaximize7RowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowmaximize"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageButton
            android:background="@drawable/rectangle_bg_bluegray_100_radius_15"
            android:id="@+id/btnMaximize"
            android:layout_height="@dimen/_30pxh"
            android:layout_width="@dimen/_30pxh"
            android:paddingHorizontal="@dimen/_9pxh"
            android:paddingVertical="@dimen/_9pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_maximize_black_900" tools:ignore="ContentDescription"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumncredittransf"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_2pxv"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_2pxv"
            android:layout_weight="0.64" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtCreditTransf"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_9pxh"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{listmaximize7RowModel.txtCreditTransf}"
                style="@style/txtWorkSansmedium12_1" tools:text="@string/msg_credit_transf"/>
            <TextView android:ellipsize="end" android:id="@+id/txtTime"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{listmaximize7RowModel.txtTime}"
                style="@style/txtWorkSansregular8_1" tools:text="@string/msg_02_12_2022_at_3"/>
        </LinearLayout>
        <LinearLayout android:gravity="end"
            android:id="@+id/linearColumnn2500000"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_2pxv"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_weight="0.24" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtN2500000"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{listmaximize7RowModel.txtN2500000}"
                style="@style/txtWorkSanssemibold12_3" tools:text="@string/lbl_n25_000_00"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPending"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{listmaximize7RowModel.txtPending}"
                style="@style/txtWorkSanssemibold8_2" tools:text="@string/lbl_pending"/>
        </LinearLayout>
    </LinearLayout>
</layout>
