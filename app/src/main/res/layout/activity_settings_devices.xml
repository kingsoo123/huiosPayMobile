<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="settingsDevicesVM" type="com.huiospayapplication.app.modules.settingsdevices.data.viewmodel.SettingsDevicesVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearSettingsDevi"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_100">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumn"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecornerRadius_13">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_21pxv"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnmenu"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:id="@+id/linearRowmenu"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMenu"
                            android:layout_height="@dimen/_10pxh"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_menu_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_menu_white_a700"/>
                        <LinearLayout
                            android:id="@+id/linearRowownyourweird"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_170pxh"
                            android:layout_weight="0.88"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <LinearLayout android:gravity="end"
                                android:id="@+id/linearColumnownyourweird"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.83"
                                android:layout_width="0dp" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:gravity="start"
                                    android:id="@+id/txtOWNYOURWEIRD"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_1pxh"
                                    android:singleLine="true"
                                    android:text="@{settingsDevicesVM.settingsDevicesModel.txtOWNYOURWEIRD}"
                                    style="@style/txtWorkSanssemibold12_2" tools:text="@string/lbl_beetle_taxi_inc"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtAcctNo001122"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_1pxh"
                                    android:layout_marginStart="@dimen/_16pxh"
                                    android:layout_marginTop="@dimen/_3pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_1pxh"
                                    android:singleLine="true"
                                    android:text="@{settingsDevicesVM.settingsDevicesModel.txtAcctNo001122}"
                                    style="@style/txtWorkSansregular10_1" tools:text="@string/msg_acct_no_001122"/>
                            </LinearLayout>
                            <FrameLayout
                                android:id="@+id/frameStackcalendar"
                                android:layout_height="@dimen/_20pxv"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_11pxh"
                                android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_20pxh">
                                <ImageView
                                    android:id="@+id/imageCalendar"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_20pxh"
                                    android:layout_width="@dimen/_20pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_calendar_white_a700"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_calendar_white_a700"/>
                                <ImageView android:id="@+id/imageTrash"
                                    android:layout_gravity="bottom|center"
                                    android:layout_height="@dimen/_10pxh"
                                    android:layout_marginEnd="@dimen/_6pxh"
                                    android:layout_marginStart="@dimen/_5pxh"
                                    android:layout_marginTop="@dimen/_10pxv"
                                    android:layout_width="@dimen/_8pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_trash_white_a700"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_trash_white_a700"/>
                            </FrameLayout>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowownyourweirdOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_28pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <LinearLayout
                            android:id="@+id/linearRowarrowleft"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.32"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageArrowleft"
                                android:layout_height="@dimen/_7pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_width="@dimen/_3pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_arrowleft"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtBack"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_weight="0.89"
                                android:layout_width="0dp"
                                android:lineSpacingExtra="@dimen/_1pxh"
                                android:singleLine="true"
                                android:text="@{settingsDevicesVM.settingsDevicesModel.txtBack}"
                                style="@style/txtWorkSansmedium12_3" tools:text="@string/lbl_back"/>
                        </LinearLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtOWNYOURWEIRDOne"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_66pxh"
                            android:layout_weight="0.68"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{settingsDevicesVM.settingsDevicesModel.txtOWNYOURWEIRDOne}"
                            style="@style/txtWorkSanssemibold24_1" tools:text="@string/lbl_settings"/>
                    </LinearLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtEnteranAccoun"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{settingsDevicesVM.settingsDevicesModel.txtEnteranAccoun}"
            style="@style/txtWorkSanssemibold20" tools:text="@string/lbl_devices"/>
        <TextView android:gravity="start" android:id="@+id/txtTime"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_38pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_2pxh"
            android:text="@{settingsDevicesVM.settingsDevicesModel.txtTime}"
            style="@style/txtWorkSansmedium16" tools:text="@string/msg_iphone_xs_last"/>
        <View android:background="@color/gray_401"
            android:id="@+id/lineLineSeventyFour"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_18pxv" android:layout_width="@dimen/_313pxh"/>
        <TextView android:gravity="start"
            android:id="@+id/txtEnterReceipien"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_2pxh"
            android:text="@{settingsDevicesVM.settingsDevicesModel.txtEnterReceipien}"
            style="@style/txtWorkSansmedium16" tools:text="@string/msg_samsung_s8_last"/>
        <View android:background="@color/gray_401"
            android:id="@+id/lineLineSeventyFive"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_18pxv" android:layout_width="@dimen/_313pxh"/>
    </LinearLayout>
</layout>
