<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listardownRowModel" type="com.huiospayapplication.app.modules.settingsposterminalfeatures.data.model.ListardownRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowarrowdown1"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <LinearLayout android:id="@+id/linearRowarrowdown2"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_weight="0.81" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageArrowdown"
                android:layout_height="@dimen/_21pxh"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowdown"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowdown"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtWithdrawal"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_weight="0.76" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_3pxh"
                android:singleLine="true"
                android:text="@{listardownRowModel.txtWithdrawal}"
                style="@style/txtWorkSansmedium12_1" tools:text="@string/lbl_withdrawal"/>
        </LinearLayout>
        <com.google.android.material.switchmaterial.SwitchMaterial
            android:id="@+id/switchMaterialComponentFourteen"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_6pxv"
            android:layout_marginStart="@dimen/_174pxh"
            android:layout_marginTop="@dimen/_6pxv"
            android:layout_weight="0.19" android:layout_width="0dp"/>
    </LinearLayout>
</layout>
