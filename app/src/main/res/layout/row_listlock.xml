<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listlockRowModel" type="com.huiospayapplication.app.modules.billpaymentselectcategory.data.model.ListlockRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowlock1"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylelight_blue_700cornerRadius">
        <LinearLayout android:id="@+id/linearRowlock2"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_31pxv"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_weight="0.85" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageLock"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="@dimen/_18pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_lock"
                tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAirtime"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_weight="0.8" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{listlockRowModel.txtAirtime}"
                style="@style/txtWorkSanssemibold20_2" tools:text="@string/lbl_utilities"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnellipsethirtythree"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_30pxv"
            android:layout_marginEnd="@dimen/_27pxh"
            android:layout_marginStart="@dimen/_137pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_weight="0.15" android:layout_width="0dp"
            android:orientation="vertical" style="@style/groupStylegray_100cornerRadius">
            <View
                android:background="@drawable/rectangle_bg_light_blue_700_radius_6_5"
                android:id="@+id/viewEllipseThirtyThree"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_13pxh"
                android:layout_marginHorizontal="@dimen/_3pxh"
                android:layout_marginVertical="@dimen/_3pxv" android:layout_width="@dimen/_13pxh"/>
        </LinearLayout>
    </LinearLayout>
</layout>
