<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="posTerminalVM" type="com.huiospayapplication.app.modules.posterminal.data.viewmodel.PosTerminalVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnrectangle5793"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_100">
        <FrameLayout android:id="@+id/frameStackrectangle5793"
            android:layout_height="@dimen/_192pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageRectangle5793"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_109pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_rectangle5793_24}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle5793_24"/>
            <LinearLayout android:id="@+id/linearColumnmenu"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearRowmenu"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMenu"
                            android:layout_height="@dimen/_10pxh"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_menu_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_menu_white_a700"/>
                        <LinearLayout
                            android:id="@+id/linearRowownyourweird"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_170pxh"
                            android:layout_weight="0.88"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <LinearLayout android:gravity="end"
                                android:id="@+id/linearColumnownyourweird"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.83"
                                android:layout_width="0dp" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:gravity="start"
                                    android:id="@+id/txtOWNYOURWEIRD"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_1pxh"
                                    android:singleLine="true"
                                    android:text="@{posTerminalVM.posTerminalModel.txtOWNYOURWEIRD}"
                                    style="@style/txtWorkSanssemibold12_2" tools:text="@string/lbl_beetle_taxi_inc"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtAcctNo001122"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_1pxh"
                                    android:layout_marginStart="@dimen/_16pxh"
                                    android:layout_marginTop="@dimen/_3pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_1pxh"
                                    android:singleLine="true"
                                    android:text="@{posTerminalVM.posTerminalModel.txtAcctNo001122}"
                                    style="@style/txtWorkSansregular10_1" tools:text="@string/msg_acct_no_001122"/>
                            </LinearLayout>
                            <FrameLayout
                                android:id="@+id/frameStackcalendar"
                                android:layout_height="@dimen/_20pxv"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_11pxh"
                                android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_20pxh">
                                <ImageView
                                    android:id="@+id/imageCalendar"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_20pxh"
                                    android:layout_width="@dimen/_20pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_calendar_white_a700"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_calendar_white_a700"/>
                                <ImageView android:id="@+id/imageTrash"
                                    android:layout_gravity="bottom|center"
                                    android:layout_height="@dimen/_10pxh"
                                    android:layout_marginEnd="@dimen/_6pxh"
                                    android:layout_marginStart="@dimen/_5pxh"
                                    android:layout_marginTop="@dimen/_10pxv"
                                    android:layout_width="@dimen/_8pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_trash_white_a700"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_trash_white_a700"/>
                            </FrameLayout>
                        </LinearLayout>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOWNYOURWEIRDOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_80pxh"
                    android:layout_marginStart="@dimen/_80pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{posTerminalVM.posTerminalModel.txtOWNYOURWEIRDOne}"
                    style="@style/txtWorkSanssemibold24_1" tools:text="@string/lbl_pos_terminal"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowellipsethirtysix"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_60pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnellipsethirtysix"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.12"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStylewhite_4">
                        <View
                            android:background="@drawable/rectangle_border_light_blue_700_radius_7_5"
                            android:id="@+id/viewEllipseThirtySix"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_15pxh"
                            android:layout_marginHorizontal="@dimen/_7pxh"
                            android:layout_marginVertical="@dimen/_7pxv" android:layout_width="@dimen/_15pxh"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumntime"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_weight="0.86"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTime"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{posTerminalVM.posTerminalModel.txtTime}"
                            style="@style/txtWorkSansmedium16" tools:text="@string/lbl_pos_2060t2am"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtEnterReceipien"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_2pxh"
                            android:singleLine="true"
                            android:text="@{posTerminalVM.posTerminalModel.txtEnterReceipien}"
                            style="@style/txtWorkSansmedium10_1" tools:text="@string/lbl_active"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowright"
                        android:layout_height="@dimen/_8pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_4pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                </LinearLayout>
                <View android:background="@color/gray_401"
                    android:id="@+id/lineLineSeventyTwo"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_20pxv" android:layout_width="@dimen/_313pxh"/>
            </LinearLayout>
        </FrameLayout>
        <LinearLayout android:id="@+id/linearRowgroup"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_349pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylegray_100">
            <ImageView android:id="@+id/imageGroup"
                android:layout_height="@dimen/_219pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="@dimen/_37pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_group_67"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group_67"/>
            <ImageView android:id="@+id/imageGroupOne"
                android:layout_height="@dimen/_219pxh"
                android:layout_marginBottom="@dimen/_23pxv"
                android:layout_marginStart="@dimen/_287pxh"
                android:layout_width="@dimen/_36pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_group_68"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group_68"/>
        </LinearLayout>
    </LinearLayout>
</layout>
