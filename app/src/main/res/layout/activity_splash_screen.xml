<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="splashScreenVM" type="com.huiospayapplication.app.modules.splashscreen.data.viewmodel.SplashScreenVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackellipseeightOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameSplashScreen"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylewhite_A700">
            <FrameLayout android:id="@+id/frameStackellipseseven"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_800pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent" style="@style/groupStyle">
                <ImageView android:id="@+id/imageEllipseSeven"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_286pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="@dimen/_320pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse7_286x320}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse7_286x320"/>
                <ImageView android:id="@+id/imageEllipseEight"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_425pxh"
                    android:layout_marginBottom="@dimen/_149pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_149pxv"
                    android:layout_width="@dimen/_225pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse8_425x225}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse8_425x225"/>
            </FrameLayout>
            <ImageView android:id="@+id/imageEllipseEightOne"
                android:layout_gravity="bottom|start"
                android:layout_height="@dimen/_411pxv"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_ellipse8_1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse8_1"/>
            <LinearLayout android:id="@+id/linearColumnvolume"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleteal_900_4c">
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowvolume"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_64pxh"
                    android:layout_marginStart="@dimen/_64pxh"
                    android:layout_marginTop="@dimen/_373pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageVolume"
                        android:layout_height="@dimen/_53pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_34pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_volume"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_volume"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtHuios"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.82"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{splashScreenVM.splashScreenModel.txtHuios}"
                        style="@style/txtWorkSansregular35" tools:text="@string/lbl_huiospay"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnGetStarted"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_75pxv"
                    android:layout_marginEnd="@dimen/_64pxh"
                    android:layout_marginStart="@dimen/_64pxh"
                    android:layout_marginTop="@dimen/_250pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_get_started"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_get_started"/>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
